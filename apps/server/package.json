{
  "name": "@vite-fast/server",
  "version": "1.0.0",
  "description": "Fastify server for ViteFast AI agent template",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@sinclair/typebox": "^0.31.28",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "@fastify/cors": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "vitest": "^1.0.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1"
  }
}
